
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@Html.Action("PartialHead","Default")
<body>
    @Html.Action("PartialNavBarInfo","Default")
    @Html.Action("PartialNavBar","Default")
    <!-- END nav -->
    @Html.Action("PartialSlider","Default")




    <section class="ftco-section ftco-wrap-about ftco-no-pb ftco-no-pt">
        <div class="container">
            <div class="row no-gutters">
                @Html.Action("PartialReservation","Default")
                
                @Html.Action("PartialAbout","Default")
            </div>
        </div>
    </section>

    <section class="ftco-section ftco-intro" style="background-image: url(images/bg_3.jpg);">
        <div class="overlay"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <span>Hemen Yerinizi Ayırın</span>
                    <h2>Private Dinners &amp; Happy Hours</h2>
                </div>
            </div>
        </div>
    </section>

    @Html.Action("PartialMenu", "Default")

    @Html.Action("PartialTestimonial","Default")

    @Html.Action("PartialChef","Default")

    @Html.Action("PartialInfo","Default")

    

    @Html.Action("PartialReservationNow","Default")

    @Html.Action("PartialFooter","Default")


    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>


    @Html.Action("PartialScript","Default")

</body>
</html>
