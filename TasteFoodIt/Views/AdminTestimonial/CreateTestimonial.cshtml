@model TasteFoodIt.Entities.Testimonial
@{
    ViewBag.Title = "CreateTestimonial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<form method="post">
    <label>İsim Soyisim</label>
    @Html.TextBoxFor(x => x.NameSurname)
    <label>Açıklama</label>
    @Html.TextBoxFor(x => x.Description)
    <label>Pozisyion</label>
    @Html.TextBoxFor(x => x.Title)
    <input type="submit" value="Güncelle" class="btn btn-success" />
    <label>Fotoğraf Yolu</label>
    @Html.TextBoxFor(X => X.ImageUrl)
</form>

